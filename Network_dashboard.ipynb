   "id": "96c69dc5-1227-494b-ba76-23e395ba8bd2",
   "metadata": {},
   "source": [
    "### Functions to create input feature for MLP-NN"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "a8ea5379-73c2-40a1-99c3-ce29c09e60b9",
   "metadata": {},
   "outputs": [],
   "source": [
    "def h_no_pump():\n",
    "    pth = r'C:***\\steady_state'\n" #Set path here, 
    "    data =np.load(os.path.join(pth,'head_no_pump.npy'))\n",
    "    h0 = data[3,:,:]  \n",
    "    return h0\n",
    "    \n",
    "\n",
    "def well_dist(row,col):    \n",
    "    xmax = 80000\n",
    "    ymax = 110000\n",
    "    X, Y = np.meshgrid(np.linspace(0,xmax,320), np.linspace(ymax,0,440))\n",
    "    phi =  -1* np.ones_like(X)\n",
    "    phi[row,col] = 1\n",
    "\n",
    "    d_well = skfmm.distance(phi,dx=250) \n",
    "    d_well = -d_well\n",
    "    return d_well\n",
    "\n",
    "def smooth_hk():\n",
    "    result = np.load('outside.npy')\n",
    "    data_hk = np.loadtxt('hyd_kon')\n",
    "    data_hk = data_hk[::-1]\n",
    "    hyk = np.flipud((data_hk))\n",
    "    hyk +=0.1\n",
    "    hk_smooth = scipy.ndimage.filters.gaussian_filter(hyk,(7,7))\n",
    "    hk_smooth[result[0,:],result[1,:]] = -2\n",
    "    return hk_smooth\n",
    "    \n",
    "\n",
    "\n",
    "def travel_time(row,col,hk_smooth):\n",
    "    data_hk = np.loadtxt('hyd_kon')\n",
    "    data_hk = data_hk[::-1]\n",
    "    hyk = np.flipud((data_hk))\n",
    "\n",
    "    xmax = 80000\n",
    "    ymax = 110000\n",
    "    X, Y = np.meshgrid(np.linspace(0,320,320), np.linspace(440,0,440))\n",
    "    phi =  np.ones_like(hyk)*hyk\n",
    "    phi[phi == 0] = 0\n",
    "    phi[phi != 0] = -1\n",
    "    phi[row,col] = 1\n",
    "\n",
    "    speed=np.ones_like(phi)*hk_smooth\n",
    "    t = skfmm.travel_time(phi, speed,dx=250)\n",
    "    return t\n",
    "\n",
    "\n",
    "def dist():\n",
    "    data_str = np.loadtxt('str.txt')\n",
    "    row = (data_str[:,1])\n",
    "    col = (data_str[:,2])\n",
    "\n",
    "    row = row.astype(int)\n",
    "    col = col.astype(int)\n",
    "\n",
    "    data_hk = np.loadtxt('hyd_kon')\n",
    "    data_hk = data_hk[::-1]\n",
    "    hyk = np.flipud((data_hk))\n",
    "\n",
    "    xmax = 80000\n",
    "    ymax = 110000\n",
    "    X, Y = np.meshgrid(np.linspace(0,xmax,320), np.linspace(ymax,0,440))\n",
    "    phi =  -1* np.ones_like(X)\n",
    "    phi[row,col] = 1\n",
    "\n",
    "    d = skfmm.distance(phi,dx=250) \n",
    "    d = -d\n",
    "    return d\n",
    "\n",
    "def data_gen(i,j,data0,pump_rate):\n",
    "    d_boundary = np.load('d_boundary.npy')\n",
    "    data_hk = np.loadtxt('hyd_kon')\n",
    "    row_nr = np.empty((440,320))\n",
    "    row_nr.fill(0)\n",
    "    col_nr = np.empty((440,320))\n",
    "    r = -1\n",
    "    c = 0\n",
    "    for k in range(440):\n",
    "        r += 1\n",
    "        c = 0\n",
    "        for h in range(320):\n",
    "            row_nr[k,h] = r\n",
    "            col_nr[k,h] = c\n",
    "            c+=1\n",
    "   \n",
    "    data_hk = data_hk[::-1]\n",
    "    hyk = np.flipud((data_hk))\n",
    "    hk_smooth = smooth_hk()\n",
    "    t = travel_time(i,j,hk_smooth)\n",
    "    d_well = well_dist(i,j)\n",
    "    d = dist()\n",
    "    \n",
    "    dists = np.reshape(d,(320*440))\n",
    "    dist_b = np.reshape(d_boundary,(320*440))\n",
    "    time = np.reshape(t,(320*440))\n",
    "    dist_well = np.reshape(d_well,(320*440))\n",
    "    head0 = np.reshape(data0,(320*440))\n",
    "    hykk = np.reshape(hyk,(320*440))\n",
    "    hykk_l = np.reshape(np.log10(hyk),(320*440))\n",
    "    pump = pump_rate*np.ones((320*440,1))\n",
    "    row = np.reshape(row_nr,(320*440))\n",
    "    col = np.reshape(col_nr,(320*440))\n",
    "    pump_roww = np.ones((320*440)) * i\n",
    "    pump_coll = np.ones((320*440)) * j\n",
    "    \n",
    "    data_set_t= pd.DataFrame(head0)\n",
    "    data_set_t.columns = [\"head\"]\n",
    "    data_set_t['dist']=dists\n",
    "    data_set_t['time']=time\n",
    "    data_set_t['dist_well']=dist_well\n",
    "    data_set_t['h_cond']=hykk\n",
    "    data_set_t['h_cond_log']=hykk_l\n",
    "    data_set_t['pump_rate'] = pump\n",
    "    data_set_t['pump_row'] = pump_roww\n",
    "    data_set_t['pump_col'] = pump_coll\n",
    "    data_set_t['row']=row\n",
    "    data_set_t['col']=col\n",
    "    data_set_t['dist_boundary']=dist_b\n",
    "    data_set_t = data_set_t[(data_set_t[['head']] != -999).all(axis=1)]\n",
    "    data_set_t = data_set_t[(data_set_t[['time']] != 0).all(axis=1)]\n",
    "    return data_set_t\n",
    "\n",
    "\n",
    "def data_scaled(row,col,h0,pump_rate):\n",
    "    scaler=joblib.load('std_scaler_new_one.bin')\n",
    "    data_set = data_gen(row,col,h0,pump_rate,)\n",
    "    scaled_data = scaler.transform(data_set.iloc[:,0:9])    \n",
    "    return scaled_data\n",
    "\n",
    "def col_check(h0,i):\n",
    "    data_hk = np.loadtxt('hyd_kon')\n",
    "    data_hk = data_hk[::-1]\n",
    "    hyk = np.flipud((data_hk))\n",
    "    values = np.where((h0[i,:]!=-999) & (h0[i,:]!=999))\n",
    "    values = np.where((hyk[i,:]>1))\n",
    "    values = np.array(values)\n",
    "    return values"
   ]
  },
