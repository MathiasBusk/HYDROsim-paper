   "source": [
    "def interactive_plot():\n",
    "    x_widget = widgets.IntSlider(min=86, max=377)\n",
    "    y_widget = widgets.Dropdown()\n",
    "    Q_wid = widgets.IntSlider(min=100, max=5000)\n",
    "    color_scale_wid = widgets.IntSlider(min=1, max=10)\n",
    "    def update_y_range(*args):\n",
    "        h0 = h_no_pump() \n",
    "        i=x_widget.value\n",
    "        data_hk = np.loadtxt('hyd_kon')\n",
    "        data_hk = data_hk[::-1]\n",
    "        hyk = np.flipud((data_hk))\n",
    "        values = np.where((h0[i,:]!=-999) & (h0[i,:]!=999))\n",
    "        values = np.where((hyk[i,:]>1))\n",
    "        values = np.array(values)\n",
    "        y_widget.options = values[0][:]\n",
    "        y_widget.value = values\n",
    "\n",
    "    x_widget.observe(update_y_range, 'value')\n",
    "\n",
    "    def printer(row, column,pumping_rate,color_scale):\n",
    "        print(row, column,pumping_rate,color_scale)\n",
    "    x_widget.observe(update_y_range, 'value')\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "    widgets.interact(printer,row=x_widget, column=y_widget,pumping_rate=Q_wid,color_scale=color_scale_wid);\n",
    "\n",
    "    button = widgets.Button(description=\"Run!\")\n",
    "    output = widgets.Output()\n",
    "\n",
    "    display(button, output)\n",
    "\n",
    "    def on_button_clicked(b):\n",
    "        with output:\n",
    "            i=x_widget.value\n",
    "            j=y_widget.value\n",
    "            j=int(j)\n",
    "            Q=Q_wid.value\n",
    "            color_scale = color_scale_wid.value\n",
    "            data0 = h_no_pump()   \n",
    "            row_len = 440\n",
    "            col_len = 320\n",
    "            means = np.zeros(24482)\n",
    "            stds = np.zeros(24482)\n",
    "\n",
    "            data_set = data_gen(i,j,data0,Q)\n",
    "            data_set_t = data_gen(i,j,data0,Q)\n",
    "            data_scale = data_scaled(i,j,data0,Q)     \n",
    "            y_hat = new_model(data_scale)\n",
    "            mean = y_hat.mean()\n",
    "            stddev = y_hat.stddev()\n",
    "            means = mean[:,0]\n",
    "            stds = stddev[:,0]\n",
    "            data_ML = np.empty((row_len,col_len))\n",
    "            data_ML.fill(np.nan)\n",
    "            data_set = np.array(data_set.iloc[:,:])\n",
    "\n",
    "            rows = data_set[:,-3].astype(int)\n",
    "            cols = data_set[:,-2].astype(int)\n",
    "            data_ML[rows,cols]=means \n",
    "\n",
    "            data_MLs = np.empty((row_len,col_len))\n",
    "            data_MLs.fill(np.nan)\n",
    "\n",
    "            data_MLs[rows,cols]=stds\n",
    "\n",
    "            d_boundary = np.load('d_boundary.npy')\n",
    "\n",
    "            #plt.rcParams.update({'font.size': 10})\n",
    "            fig, (ax1,ax2) = plt.subplots(1, 2,figsize=(20,10))\n",
    "            plt.rcParams.update({'font.size': 25})\n",
    "\n",
    "\n",
    "            ax1.set_title('Mean head change')\n",
    "            c = ax1.imshow(data_ML,cmap='gist_heat_r',vmin=0,vmax=color_scale)\n",
    "\n",
    "            cbar=plt.colorbar(c,ax=ax1)\n",
    "            cbar.set_label('Head change [m]')\n",
    "            ax1.contour(d_boundary, 0,colors='black')\n",
    "            ax1.set_xlabel('column')\n",
    "            ax1.set_ylabel('row')\n",
    "\n",
    "            ax2.set_title('Standard deviation')\n",
    "            c2 = ax2.imshow(data_MLs,cmap='gist_heat_r',vmin=0,vmax=1)\n",
    "            cbar2=plt.colorbar(c2)\n",
    "            cbar2.set_label('Std [m]')\n",
    "            ax2.contour(d_boundary, 0,colors='black')\n",
    "            ax2.set_xlabel('column')\n",
    "            ax2.set_ylabel('row')\n",
    "            plt.show()\n",
    "            clear_output(wait=True)\n",
    "\n",
    "    button.on_click(on_button_clicked)\n",
    "    \n"
   ]
